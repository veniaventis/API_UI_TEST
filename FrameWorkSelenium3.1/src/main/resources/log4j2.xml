<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn">
    <Appenders>
        <File name="File" fileName="logs/test.log" append="false">
            <PatternLayout>
                <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
            </PatternLayout>
        </File>
    </Appenders>
    <Loggers>
        <Logger name="FileLogger" level="trace">
            <AppenderRef ref="File"/>
        </Logger>
    </Loggers>
</Configuration>